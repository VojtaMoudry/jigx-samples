          tags:
            -  title: 'Verbal'
               color: color6
          




data: =@ctx.data.meetings

item: 
  componentId: "@jigx/jc-event"
  options:
    title: =@ctx.parents.list-data.item.title
    location: =@ctx.parents.list-data.item.location
    description: =@ctx.parents.list-data.item.description
    from: =@ctx.parents.list-data.item.from
    to: =@ctx.parents.list-data.item.to
    people: =@ctx.parents.list-data.item.people
    tags: =@ctx.parents.list-data.item.tags