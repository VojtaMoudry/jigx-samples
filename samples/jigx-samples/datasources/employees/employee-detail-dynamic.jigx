type: 'datasource.sqlite'
options:
  provider: DATA_PROVIDER_DYNAMIC

  entities:
    - entity: default/employees

  query: SELECT id, '$.firstname', '$.lastname', '$.photo', '$.birth-date', '$.gender', '$.email', '$.phone', '$.street', '$.city', '$.state', '$.country', '$.category' FROM [default/employees] WHERE '$.category' = "employee-detail"
