type: datasource.sqlite
options:
  provider: DATA_PROVIDER_DYNAMIC
  entities:
    - entity: default/avatar
  query: |
    SELECT
      '$.id',
      '$.title',
      '$.description',
      '$.image',
      '$.thumbnail',
      '$.url',
      '$.videoUrl',
      '$.category'
    FROM [default/avatar] WHERE '$.category' = "image-source"