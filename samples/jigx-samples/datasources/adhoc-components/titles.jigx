type: datasource.sqlite
options:
  provider: DATA_PROVIDER_DYNAMIC
  entities:
    - entity: default/location
  query: |
    SELECT
      '$.id',
      '$.title',
      '$.image',
      '$.address',
      '$.locationTitle',
      '$.locationSubtitle',
      '$.category'
    FROM [default/location] WHERE '$.category' = "titles"