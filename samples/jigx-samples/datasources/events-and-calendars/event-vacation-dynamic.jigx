type: datasource.sqlite
options:
  provider: DATA_PROVIDER_DYNAMIC
  entities:
    - entity: default/vacation-event
  query: |
    SELECT
      '$.name',
      '$.location',
      '$.date-from',
      '$.date-to',
      '$.category' 
    FROM [default/vacation-event] WHERE '$.category' = 'meeting-event'