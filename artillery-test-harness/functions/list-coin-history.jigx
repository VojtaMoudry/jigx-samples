method: GET
provider: DATA_PROVIDER_REST
parameters:
  symbol:
    location: path
    type: string
    value: BTC-USD
    required: true
  granularity:
    location: query
    type: number
    value: 60
    required: true
url: 'https://api.pro.coinbase.com/products/{symbol}/candles'
outputTransform: >-
  $.{ "ms": $[0] * 1000, "close": $formatNumber($number($[4]), '#.00000000') }

#$.{ "date": $fromMillis($[0]), "close": $formatNumber($number($[4]), '#.00000000') }
